{
  "name": "frontend",
  "version": "0.0.1",
  "description": "Frontend",
  "main": "index.js",
  "scripts": {
    "webpack": "npx webpack --env.BROWSERS=module --config webpack.dev.js",
    "watch": "npx webpack --watch --env.BROWSERS=module --config webpack.dev.js",
    "start": "npx webpack-dev-server --open --env.BROWSERS=module --config webpack.dev.js --port 8000 --history-api-fallback",
    "build": "npx npm-run-all --serial build:webpack build:webpack:no-module",
    "build:webpack": "npx webpack --env.BROWSERS=module --mode production --config webpack.prod.js",
    "build:webpack:no-module": "npx webpack --env.BROWSERS=no-module --mode production --config webpack.nomodule.js",
    "jest": "npx jest",
    "karma": "npx karma start --coverage",
    "test": "npm run karma && npm run jest",
    "test:watch": "karma start --auto-watch=true --single-run=false",
    "test:update-snapshots": "karma start --update-snapshots",
    "test:prune-snapshots": "karma start --prune-snapshots",
    "plop": "npm run npx:plop",
    "npx:plop": "npx plop",
    "create": "npm run plop",
    "create:doc": "npm run plop -- create:doc",
    "create:file": "npm run plop -- create:file",
    "release": "npx standard-version",
    "push:tags": "git push --follow-tags origin master",
    "install:docs": "git submodule add --force https://github.com/thingsym/hugo-theme-techdoc.git hugo/themes/techdoc",
    "serve:docs": "hugo serve",
    "build:docs": "hugo && npx typedoc --out public/code-js"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:senti-techlabs/products/core/experiments/web-application-experiment/frontend.git"
  },
  "keywords": [
    "base-template"
  ],
  "author": "Senti Techlabs Inc.",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/plugin-external-helpers": "7.10.4",
    "@babel/plugin-proposal-class-properties": "7.10.4",
    "@babel/plugin-proposal-decorators": "7.10.5",
    "@babel/plugin-proposal-object-rest-spread": "7.11.0",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@babel/runtime": "7.11.2",
    "@commitlint/cli": "11.0.0",
    "@commitlint/config-conventional": "11.0.0",
    "@microsoft/tsdoc": "0.12.21",
    "@open-wc/semantic-dom-diff": "^0.17.10",
    "@open-wc/testing": "^2.5.25",
    "@open-wc/testing-helpers": "^1.8.9",
    "@open-wc/testing-karma": "^4.0.5",
    "@types/jsdoc-to-markdown": "^6.0.0",
    "@types/marked": "^1.1.0",
    "@types/open-wc__testing-karma": "^3.3.0",
    "@types/quill": "^2.0.4",
    "@typescript-eslint/eslint-plugin": "4.2.0",
    "babel-eslint": "10.1.0",
    "babel-loader": "^8.1.0",
    "bundlesize": "^0.18.0",
    "chai": "^4.2.0",
    "change-case": "4.1.1",
    "clean-webpack-plugin": "^3.0.0",
    "conventional-changelog": "3.1.23",
    "copy-webpack-plugin": "^6.1.1",
    "css-loader": "^4.2.2",
    "deepmerge": "^4.2.2",
    "ejs-loader": "^0.5.0",
    "eslint": "7.10.0",
    "eslint-config-semistandard": "15.0.1",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-chai-expect": "2.2.0",
    "eslint-plugin-chai-friendly": "0.6.0",
    "eslint-plugin-html": "6.1.0",
    "eslint-plugin-import": "2.22.0",
    "eslint-plugin-mocha": "8.0.0",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.1",
    "favicons-webpack-plugin": "^4.2.0",
    "firebase-tools": "^8.12.1",
    "html-inline-css-webpack-plugin": "^1.9.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "4.3.0",
    "inquirer-directory": "2.2.0",
    "is-invalid-path": "1.0.2",
    "jest": "^26.4.2",
    "jest-cli": "^26.4.2",
    "jest-puppeteer": "^4.4.0",
    "karma": "^5.1.1",
    "karma-webpack": "^4.0.2",
    "mini-css-extract-plugin": "^0.11.2",
    "mocha": "^8.1.3",
    "npm-run-all": "4.1.5",
    "plop": "2.7.4",
    "puppeteer": "^5.2.1",
    "raw-loader": "^4.0.2",
    "sass": "^1.26.11",
    "sass-loader": "^10.0.2",
    "script-ext-html-webpack-plugin": "^2.1.4",
    "semistandard": "14.2.3",
    "sinon": "^9.0.3",
    "standard-version": "9.0.0",
    "style-loader": "^1.2.1",
    "tap": "^14.10.8",
    "typedoc": "0.19.2",
    "typescript": "4.0.3",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-merge": "^5.1.2",
    "webpack-visualizer-plugin": "^0.1.11",
    "workbox-webpack-plugin": "^5.1.3",
    "worker-loader": "^3.0.2"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "semistandard": {
    "plugins": [
      "html",
      "standard",
      "promise",
      "mocha",
      "html",
      "@typescript-eslint",
      "chai-expect"
    ],
    "parser": "babel-eslint",
    "env": [
      "mocha"
    ],
    "ignore": [
      "example/dist",
      "dist",
      "dist/**",
      "public",
      "docs",
      "hugo"
    ],
    "globals": [
      "describe",
      "it",
      "after",
      "before",
      "expect",
      "self"
    ]
  },
  "sentiProjectTemplate": {
    "version": "0.0.3",
    "templateName": "Base Template",
    "templateRepo": "git@gitlab.com:senti-techlabs/templates/base-template.git"
  },
  "dependencies": {
    "@tjmonsi/lit-router": "0.0.16",
    "@vaadin/vaadin-rich-text-editor": "^1.3.0",
    "@webcomponents/webcomponentsjs": "^2.4.4",
    "comlink": "^4.3.0",
    "dexie": "^3.0.2",
    "es6-symbol": "^3.1.3",
    "firebase": "^7.23.0",
    "lit-element": "^2.4.0",
    "lit-html": "^1.3.0",
    "marked": "^1.2.0",
    "proxy-polyfill": "^0.3.2",
    "quill": "2.0.0-dev.4"
  }
}
